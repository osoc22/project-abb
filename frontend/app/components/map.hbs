<LeafletMap
  id='map-cursor'
  @lat={{this.lat}}
  @lng={{this.lng}}
  @zoom={{this.zoom}}
  @onClick={{this.onMapClick}}
  @maxBounds={{this.bounds}}
  as |layers|
>
  {{! <layers.geojson @geoJSON='{this.geoData}' /> }}
  <layers.geojson
    @geoJSON={{this.geoData}}
    @style={{this.style}}
    @onEachFeature={{this.onEachFeature}}
  />
  {{! <layers.tile @url='https://tile.openstreetmap.be/osmbe/{z}/{x}/{y}.png' /> }}
</LeafletMap>
